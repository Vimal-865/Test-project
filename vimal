#Test

let n = 5;
let string = "";
let m = n;

for (i = 0; i < n; i++){
    for (j = 0; j < n; j++){
        if(j === 0 || j === n-1){
            string += "*"
        }
        else if (j == m - 5) {
            string +="*"
        }
        else{
            string += " "
        }
    }
    m++;
    string += "\n";
}

console.log(string);
